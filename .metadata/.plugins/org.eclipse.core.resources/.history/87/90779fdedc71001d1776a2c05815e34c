import java.text.Format;

import entities.Product;

public class Application {

	public static void main(String[] args) {
		
		
		
		Product prod = new Product("celular", 1000.00, 2);
		
		System.out.println(String.format("celular %s" , prod.getNome()));
		System.out.println(String.format("celular %d" , prod.getNome()));
		
		
	}

}
